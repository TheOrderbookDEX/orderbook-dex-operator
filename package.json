{
  "name": "@theorderbookdex/orderbook-dex-operator",
  "version": "1.0.0-rc.18",
  "description": "This is the package for the Operator smart contract of The Orderbook DEX",
  "license": "BUSL-1.1",
  "author": "The Orderbook DEX Team",
  "main": "dist/index.js",
  "repository": "https://github.com/TheOrderbookDEX/orderbook-dex-operator.git",
  "scripts": {
    "clean": "del artifacts \"src/!(tsconfig.json)\" dist",
    "solc": "wsolc contracts --optimize --evmVersion paris --save-input-json --output-dir artifacts",
    "abi2ts": "abi2ts artifacts --output-dir src",
    "compile": "tsc -p src",
    "build": "run-s clean solc abi2ts compile",
    "prepare": "npm run build",
    "test": "mocha",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@frugalwizard/abi2ts-lib": "^0.8.1",
    "@frugalwizard/addressbook": "1.7.0",
    "@openzeppelin/contracts": "4.8.3",
    "@theorderbookdex/orderbook-dex": "1.0.0-rc.17"
  },
  "devDependencies": {
    "@frugalwizard/abi2ts": "^0.9.1",
    "@frugalwizard/contract-test-helper": "^0.6.0",
    "@frugalwizard/solidity-compiler-wrapper": "^1.3.0",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-string": "^1.4.2",
    "@types/mocha": "^10.0.1",
    "@types/mochawesome": "^6.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.5.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.40.0",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "npm-run-all": "^4.1.5",
    "solc": "0.8.20",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
